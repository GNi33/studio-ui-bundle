{
  "compilerOptions": {
    "module": "es6",
    "moduleResolution": "node",
    "baseUrl": "./assets/js/",
    "paths": {
      "@Pimcore/*": ["./src/*"],
      "@test-utils/*": ["./test-utils/*"]
    },
    "strictNullChecks": true,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}
